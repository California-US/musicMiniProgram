.musicListSquareContainer {
  height: 100%;
  width: 100%;
  background-color: #f9f9f9;
}

.head {
   width: 100%;
  height: 60rpx;
  display: flex;
  align-items: center;
  background: -webkit-linear-gradient(top, #ff6400, #ff802b, #ff9047); 
  white-space: nowrap;
  font-size: 25rpx;
}

.tags {
  white-space: nowrap;
  overflow: hidden;
  width: 100%;
  margin: 20rpx 0 20rpx 32rpx;
  display: flex;
  align-items: center;
}

.tagItem {
  display: inline-block;
  margin: 0rpx 32rpx;
  color: #eee;
  font-size: 28rpx;
}

.tagItem:nth-child(1){
  margin:0 32rpx 0 0;
}

.moreTag {
  width: 10%;
  height: 60rpx; 
  line-height: 65rpx;
  text-align: center;
}



.body {
  margin-top: 20rpx;
}


.selected {
  color: black;
  font-weight: 600;
  padding: 5rpx 20rpx;
  border-radius: 20rpx;
  color:#fff;
  background-color: #ffa061;
}

.categorySwiper,
.categorySwiperItem {
  width: 100%;
  height: calc(100vh - 100rpx);
}

.returnButton {
  margin:30rpx;
  display: flex;
  align-items: center;
}

.icon-fanhui {
  font-size: 40rpx;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
}

.info {
  margin-left: 20rpx;
  font-size: 30rpx;
  color: rgb(78, 78, 78);
}

.loadingContainer {
  width: 100%;
  text-align: center;
  margin-bottom: 130rpx;
  margin-top: 50rpx;
}

.head .icon-fanhui {
  color: rgb(114, 114, 114);
  margin-left: 20rpx;
}

.tagName {
  margin-left: 10rpx;
  font-size: 32rpx;
  color: rgb(70, 69, 69);
  line-height: 60rpx;
}

.mask {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: calc(100vh - 110rpx);
  z-index: 10000;
}
