/* 开屏动画 */
.start {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 100000;
  background: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 20%;
  background-color: #ff6400;
  background-position: 50% 25%;
}

.start .text {
  position: absolute;
  width: 100%;
  font-size: 40rpx;
  color: #fff;
  position: absolute;
  top: 35%;
  text-align: center;
  font-weight: 600;
}

mp-loading {
  height: 50rpx;
  width: 100%;
  position: absolute;
  top: 75%;
}

/* 公共容器 */
.commonContainer {
  padding: 10rpx 20rpx 20rpx;
}

/* 分割线 */
.borderLine {
  border-bottom: 20rpx solid #eee;
}

/* 搜索框 */
.header {
  display: flex;
  background: -webkit-linear-gradient(top, #ff6400, #ff8d41);
}

.header .search {
  border: 1rpx solid #eee;
  flex: 1;
  margin: 20rpx 50rpx;
  height: 60rpx;
  font-size: 26rpx;
  text-align: center;
  line-height: 60rpx;
  color: #939594;
  background-color: #edefee;
  border-radius: 30rpx;
}

/* 自定义轮播图指示点 */
.swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: -5rpx;
}

.swiper-box .wx-swiper-dot {
  width: 18rpx;
  display: inline-flex;
  height: 4rpx;
  margin-left: -10rpx;
  justify-content: space-between;
}

.swiper-box .wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  border-radius: 8rpx;
  background: rgba(255, 255, 255, 0.8);
}

.swiper-box .wx-swiper-dot-active::before {
  background: #ff6400;
}


/* 轮播图区域 */
.banners {
  width: 100%;
  height: 310rpx;
  padding: 10rpx 20rpx 24rpx;
  box-sizing: border-box;
  background: -webkit-linear-gradient(top, #ff8d41, #ffb380);
}

.banners image {
  width: 100%;
  border-radius: 18rpx;
}

.bannerTitle {
  position: absolute;
  color: #fff;
  bottom: 0;
  right: 0;
  font-size: 22rpx;
  padding: 1px 18rpx;
  border-radius: 18rpx 0 10rpx 0;
  /* opacity: 0.8; */
}

/* 五个图标导航区域*/
.navContainer {
  display: flex;
  padding: 0 20rpx 20rpx;
  border-bottom: 2rpx solid #eee;
  background: -webkit-linear-gradient(top, #ffb380, #ffdac2, #fff);
  justify-content: space-around;
}

.navItem {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 20%;
}

.navItem .iconfont {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 100rpx;
  background: rgb(255, 100, 0);
  font-size: 50rpx;
  color: #fff;
  margin: 20rpx 0;
}

.navItem text {
  font-size: 26rpx;
}

/* 推荐歌单区域*/
.recommendScroll {
  display: flex;
  height: 300rpx;
}

.scrollItem {
  width: 200rpx;
  margin-left: 20rpx;
}

.scrollItem image {
  width: 198rpx;
  height: 198rpx;
  border-radius: 20rpx;
  border: 2rpx solid #eee;
}

.scrollItem text {
  font-size: 26rpx;
  /* 单行文本溢出隐藏 */
  /* display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis; */

  /* 多行文本溢出隐藏 */
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  /* 设置对齐模式 */
  -webkit-box-orient: vertical;
  /* 设置多行行数 */
  -webkit-line-clamp: 2;
}

/* 排行榜区域 */
.topListSwiper {
  height: 420rpx;
}

.swiperItem {
  width: 96%;
}

.swiperItem .title {
  margin: 0 20rpx 20rpx;
}

/* 最新歌曲区域 */
.musicItem {
  position: relative;
  display: flex;
  /* 元素设置flex，子元素自动成为block */
  margin: 0 20rpx 20rpx;
}

.musicItem image {
  width: 96rpx;
  height: 96rpx;
  border-radius: 12rpx;
  border: 2rpx solid #eee;
}


.musicInfo {
  display: flex;
  flex-direction: column;
  flex: 1;
  height: 98rpx;
  border-bottom: 2rpx solid #eee;
  margin-left: 30rpx;
}

.musicInfo .musicName {
  width: 460rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  padding-top: 6rpx;
}

.musicInfo .musicAuthor {
  width: 460rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.musicInfo .musicAuthor text {
  font-size: 26rpx;
  color: #777;
}

.tip {
  font-size: 32rpx;
  color: #aaa;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
}

.icon-gengduo {
  position: absolute;
  right: 0;
  font-size: 40rpx;
  line-height: 100rpx;
  width: 50rpx;
  height: 100rpx;
  text-align: center;
  color: #9f9f9f;
}
